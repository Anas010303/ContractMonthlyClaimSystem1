@model IEnumerable<Claim>
<h2>Approved Claims Report</h2>
<table>
    <thead>
        <tr>
            <th>Lecturer</th>
            <th>Total Payment</th>
            <th>Submission Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.LecturerName</td>
                <td>@claim.TotalPayment</td>
                <td>@claim.SubmissionDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
